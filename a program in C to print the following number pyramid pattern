#include <stdio.h>

int main()
{
    int i, j, k;
    for (i = 1; i <= 5; i++) {
        for (j = 5 - i; j >= 1; j--) {
            printf(" ");
        }
        for (k = i; k >= 1; k--) {
            printf("%d", k);
        }
        for (k = 2; k <= i; k++) {
            printf("%d", k);
        }
        printf("\n");
    }
    return 0;
}
